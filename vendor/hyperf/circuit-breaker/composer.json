{
    "name": "hyperf/circuit-breaker",
    "description": "A circuit breaker component for hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "circuit breaker"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=7.2",
        "psr/container": "^1.0",
        "hyperf/utils": "~2.1.0"
    },
    "require-dev": {
        "hyperf/contract": "~2.1.0",
        "hyperf/di": "~2.1.0",
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
        "hyperf/contract": "Required to use annotations.",
        "hyperf/di": "Required to use annotations."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\CircuitBreaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\CircuitBreaker\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
