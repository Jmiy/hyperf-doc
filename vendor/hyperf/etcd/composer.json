{
    "name": "hyperf/etcd",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "etcd"
    ],
    "description": "Etcd Client for Hyperf.",
    "autoload": {
        "psr-4": {
            "Hyperf\\Etcd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Etcd\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "hyperf/guzzle": "~2.1.0",
        "hyperf/utils": "~2.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "suggest": {
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Etcd\\ConfigProvider"
        }
    }
}
